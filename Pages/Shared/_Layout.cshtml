<!DOCTYPE html>
<html
    lang = "en">
<head>
    <meta
        charset = "utf-8"/>
    <meta
        content = "width=device-width, initial-scale=1.0"
        name = "viewport"/>
    <title>@ViewData["Title"] - BugTracker</title>
    <link
        href = "~/lib/bootstrap/dist/css/bootstrap.min.css"
        rel = "stylesheet"/>
    <link
        href = "~/css/site.css"
        rel = "stylesheet"/>
</head>
<body>
<header>
    <nav
        class = "bg-white border-bottom box-shadow mb-3 navbar navbar-expand-sm navbar-light navbar-toggleable-sm">
        <div
            class = "container">
            <!-- Icon -->
            <svg
                class = "bi bi-bug-fill"
                fill = "currentColor"
                height = "16"
                viewBox = "0 0 16 16"
                width = "16"
                xmlns = "http://www.w3.org/2000/svg">
                <path
                    d = "M4.978.855a.5.5 0 1 0-.956.29l.41 1.352A4.985 4.985 0 0 0 3 6h10a4.985 4.985 0 0 0-1.432-3.503l.41-1.352a.5.5 0 1 0-.956-.29l-.291.956A4.978 4.978 0 0 0 8 1a4.979 4.979 0 0 0-2.731.811l-.29-.956z"/>
                <path
                    d = "M13 6v1H8.5v8.975A5 5 0 0 0 13 11h.5a.5.5 0 0 1 .5.5v.5a.5.5 0 1 0 1 0v-.5a1.5 1.5 0 0 0-1.5-1.5H13V9h1.5a.5.5 0 0 0 0-1H13V7h.5A1.5 1.5 0 0 0 15 5.5V5a.5.5 0 0 0-1 0v.5a.5.5 0 0 1-.5.5H13zm-5.5 9.975V7H3V6h-.5a.5.5 0 0 1-.5-.5V5a.5.5 0 0 0-1 0v.5A1.5 1.5 0 0 0 2.5 7H3v1H1.5a.5.5 0 0 0 0 1H3v1h-.5A1.5 1.5 0 0 0 1 11.5v.5a.5.5 0 1 0 1 0v-.5a.5.5 0 0 1 .5-.5H3a5 5 0 0 0 4.5 4.975z"/>
            </svg>
            <a
                asp-area = ""
                asp-page = "/Login"
                class = "navbar-brand">
                BugTracker
            </a>
            <button
                aria-controls = "navbarSupportedContent"
                aria-expanded = "false"
                aria-label = "Toggle navigation"
                class = "navbar-toggler"
                data-target = ".navbar-collapse"
                data-toggle = "collapse"
                type = "button">
                <span
                    class = "navbar-toggler-icon">
                </span>
            </button>
            <div
                class = "collapse d-sm-inline-flex flex-sm-row-reverse navbar-collapse">
                <ul
                    class = "flex-grow-1 navbar-nav">
                    <li
                        style = "horiz-align: center">
                        <svg
                            class = "bi bi-tags-fill"
                            fill = "currentColor"
                            height = "16"
                            viewBox = "0 0 16 16"
                            width = "16"
                            xmlns = "http://www.w3.org/2000/svg">
                            <path
                                d = "M2 2a1 1 0 0 1 1-1h4.586a1 1 0 0 1 .707.293l7 7a1 1 0 0 1 0 1.414l-4.586 4.586a1 1 0 0 1-1.414 0l-7-7A1 1 0 0 1 2 6.586V2zm3.5 4a1.5 1.5 0 1 0 0-3 1.5 1.5 0 0 0 0 3z"/>
                            <path
                                d = "M1.293 7.793A1 1 0 0 1 1 7.086V2a1 1 0 0 0-1 1v4.586a1 1 0 0 0 .293.707l7 7a1 1 0 0 0 1.414 0l.043-.043-7.457-7.457z"/>
                        </svg>
                    </li>
                    <li
                        class = "nav-item">
                        <a
                            asp-area = ""
                            asp-page = "/Tickets"
                            class = "nav-link text-dark">
                            My Tickets
                        </a>
                    </li>
                    <li>
                        <svg
                            class = "bi bi-tag-fill"
                            fill = "currentColor"
                            height = "16"
                            viewBox = "0 0 16 16"
                            width = "16"
                            xmlns = "http://www.w3.org/2000/svg">
                            <path
                                d = "M2 1a1 1 0 0 0-1 1v4.586a1 1 0 0 0 .293.707l7 7a1 1 0 0 0 1.414 0l4.586-4.586a1 1 0 0 0 0-1.414l-7-7A1 1 0 0 0 6.586 1H2zm4 3.5a1.5 1.5 0 1 1-3 0 1.5 1.5 0 0 1 3 0z"/>
                        </svg>
                    </li>
                    <li
                        class = "nav-item">
                        <a
                            asp-area = ""
                            asp-page = "/CreateTicket"
                            class = "nav-link text-dark">
                            New Ticket
                        </a>
                    </li>
                    <li>
                        <svg
                            class = "bi bi-people-fill"
                            fill = "currentColor"
                            height = "16"
                            viewBox = "0 0 16 16"
                            width = "16"
                            xmlns = "http://www.w3.org/2000/svg">
                            <path
                                d = "M7 14s-1 0-1-1 1-4 5-4 5 3 5 4-1 1-1 1H7zm4-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
                            <path
                                d = "M5.216 14A2.238 2.238 0 0 1 5 13c0-1.355.68-2.75 1.936-3.72A6.325 6.325 0 0 0 5 9c-4 0-5 3-5 4s1 1 1 1h4.216z"
                                fill-rule = "evenodd"/>
                            <path
                                d = "M4.5 8a2.5 2.5 0 1 0 0-5 2.5 2.5 0 0 0 0 5z"/>
                        </svg>
                    </li>
                    <li>
                        <a
                            asp-page = "/Users"
                            class = "nav-link text-dark">
                            Users
                        </a>
                    </li>
                    <li>
                        <svg
                            class = "bi bi-person-plus-fill"
                            fill = "currentColor"
                            height = "16"
                            viewBox = "0 0 16 16"
                            width = "16"
                            xmlns = "http://www.w3.org/2000/svg">
                            <path
                                d = "M1 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1H1zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6z"/>
                            <path
                                d = "M13.5 5a.5.5 0 0 1 .5.5V7h1.5a.5.5 0 0 1 0 1H14v1.5a.5.5 0 0 1-1 0V8h-1.5a.5.5 0 0 1 0-1H13V5.5a.5.5 0 0 1 .5-.5z"
                                fill-rule = "evenodd"/>
                        </svg>
                    </li>
                    <li
                        class = "nav-item">
                        <a
                            asp-page = "/CreateUser"
                            class = "nav-link text-dark">
                            Create User
                        </a>
                    </li>
                </ul>
            </div>
        </div>
    </nav>
</header>
<div
    class = "container">
    <main
        class = "pb-3"
        role = "main">
        @RenderBody()
    </main>
</div>

<footer
    class = "border-top footer text-muted">
    <div
        class = "container">
    </div>
</footer>

<script
    src = "~/lib/jquery/dist/jquery.min.js"></script>
<script
    src = "~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
@RenderSection("Scripts", required: false)
</body>
</html>